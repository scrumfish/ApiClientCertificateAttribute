    <metadata>
        <id>Scrumfish-WebApi2-Client-Certificte-Filter</id>
        <version>1.0.0.0-rc1</version>
        <title>Web API 2 Client Certificate Filter</title>
        <authors>Steve Reece</authors>
        <owners>Steve Reece</owners>
        <licenseUrl>https://github.com/scrumfish/ApiClientCertificateAttribute/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/scrumfish/ApiClientCertificateAttribute</projectUrl>
